    <%= javascript_include_tag "vendor/slick/slick.min.js" %>
    <%= javascript_include_tag "js/slick-custom.js" %>
    <%= javascript_include_tag "vendor/parallax100/parallax100.js" %>
    <%= javascript_include_tag "vendor/countdowntime/countdowntime.js" %>
    <%= javascript_include_tag "vendor/lightbox2/js/lightbox.min.js" %>
    <%= javascript_include_tag "js/main.js" %>
  </body>
</html>
