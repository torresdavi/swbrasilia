<!DOCTYPE html>
<html>
  <head>
    <title>Barzil</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag "fonts/font-awesome-4.7.0/css/font-awesome.min.css" %>
    <%= stylesheet_link_tag "fonts/themify/themify-icons.css" %>
    <%= stylesheet_link_tag "vendor/animate/animate.css" %>
    <%= stylesheet_link_tag "vendor/css-hamburgers/hamburgers.min.css" %>
    <%= stylesheet_link_tag "vendor/animsition/css/animsition.min.css" %>
    <%= stylesheet_link_tag "vendor/select2/select2.min.css" %>
    <%= stylesheet_link_tag "vendor/daterangepicker/daterangepicker.css" %>
    <%= stylesheet_link_tag "vendor/slick/slick.css" %>
    <%= stylesheet_link_tag "vendor/lightbox2/css/lightbox.min.css" %>
    <%= stylesheet_link_tag "css/util.css" %>
    <%= stylesheet_link_tag "css/main.css" %>
  </head>

  <body>
    <%= yield %>
    <%= javascript_include_tag "vendor/jquery/jquery-3.2.1.min.js" %>
    <%= javascript_include_tag "vendor/animsition/js/animsition.min.js" %>
    <%= javascript_include_tag "vendor/bootstrap/js/popper.js" %>
